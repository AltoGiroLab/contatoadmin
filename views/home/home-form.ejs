<script>

    function actualPage() {
        //document.getElementById("btn_prev").style.visibility = 'hidden';
        document.getElementById("pageNumber").setAttribute('value', current_page)
    }

    function firstPage() {
        current_page = 1
        changePage()
        listJson()
        actualPage()
    }

    function prevPage() {
        if (current_page > 1) {
            current_page--;
            changePage();
            listJson()
            actualPage()
        }
    }

    function nextPage() {
        if (current_page < total_pages) {
            current_page++;
            changePage();
            listJson()
            actualPage()
        }
    }

    function lastPage() {
        current_page = total_pages
        changePage()
        listJson()
        actualPage()
    }

    function changePage() {
        var btn_next = document.getElementById("btn_next");
        var btn_prev = document.getElementById("btn_prev");
        var listing_table = document.getElementById("tableList");
        if (current_page < 1) {
            current_page = 1;
        }

        btn_first.disabled = !(current_page > 1)
        btn_prev.disabled = !(current_page > 1)
        btn_next.disabled = (current_page === total_pages)
        btn_last.disabled = (current_page === total_pages)
    }

    window.onload = function () {
        changePage(1);
        listJson()
        actualPage()
    };

</script>